  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Mahonkin.TeaDataService" Version="1.3.1" />
    <PackageReference Include="Mahonkin.UnifiedLogging" Version="0.0.45" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="8.0.0" />
  </ItemGroup>
</Project>
